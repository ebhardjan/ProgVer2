// tests weather we handle comparisons correctly (negative case)
// expected assertion-might-fail-count: 8
method comparisons() {
    var a: Int
    var b: Int

    assume a == 2
    assume b == 3

    assert a == b
    assert a != a

    assert a > b
    assert a >= b
    assert !(a >= a)

    assert b < a
    assert b <= a
    assert !(b <= b)
}
